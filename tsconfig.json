{
	"compilerOptions": {
		"outDir": "./dist",
		"allowJs": true,
		"module": "commonjs",
		"target": "es2017",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"skipLibCheck": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules", "unused"]
}
